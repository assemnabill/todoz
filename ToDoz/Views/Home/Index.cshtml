@{
    ViewData["Title"] = "Home Page";
    //Layout = "_Layout";

}


<!-- //header -->
    <header>
	<div class="container">
		<!-- nav -->
		<nav class="py-4 d-lg-flex">
			<div id="logo">
                <img src="~/assets/images/logo.png" />
			</div>
			<label for="drop" class="toggle"><span class="fa fa-bars" data-selector="span.fa" style="outline: none; cursor: inherit;"></span></label>
			<input type="checkbox" id="drop">
			<ul class="menu mt-md-2 ml-auto">
				<li class="mr-lg-4 mr-2 active"><a asp-controller="Home" asp-action="Index" data-selector="nav a" style="">Home</a></li>
				
				<li class="mr-lg-4 mr-2"><a href="#features" data-selector="nav a" style="color: rgb(255, 255, 255); font-weight: 400; text-transform: capitalize;">Features</a></li>
				<li class="mr-lg-4 mr-2"><a href="#how" data-selector="nav a" style="color: rgb(255, 255, 255); font-weight: 400; text-transform: capitalize;">How it Works</a></li>
				
				<li class="mr-lg-4 mr-2"><a href="#contact" data-selector="nav a" style="">Contact</a></li>

                 @if (User.Identity.IsAuthenticated)
             {
                  <li class="mr-lg-4 mr-2"><a data-selector="nav a" asp-controller="Account" asp-action="Profile" style="color: rgb(255, 255, 255); font-weight: 400; text-transform: capitalize;">Hi @User.Identity.Name!</a></li>
                  <li class="login-icon">
				    <a class="user btn" asp-controller="Account" asp-action="Logout" data-selector="a.btn, button.btn" style="border-radius: 0px; font-size: 13px; background-color: rgb(32, 201, 151);">SIGN OUT</a>
			     </li>
             }
             else
             {
                   <li class="login-icon">
				        <a class="user btn" asp-controller="Account" asp-action="Login" data-selector="a.btn, button.btn" style="border-radius: 0px; font-size: 13px; background-color: rgb(32, 201, 151);">SIGN IN</a>
			       </li>
             }
			</ul>
            
		</nav>
		<!-- //nav -->
	</div>
        </header>
<!-- //header -->

<!-- banner -->
<section class="position-relative">
	<div class="banner w3pvt-banner bg bg1" id="home" data-selector=".bg.bg1, .bg.bg2, .header10, .header11" style="background-color: rgba(0, 0, 0, 0); margin-top: 0px; background-image: url(&quot;../assets/images/authbgimage.jpg&quot;);">
