                                @if (User.Identity.IsAuthenticated) {
                                    
                                    <a href="~/Dashboard/index" class="btn mr-2" data-selector="a.btn, button.btn" style="">Dashboard</a>
		                            <a href="#services" class="btn" data-selector="a.btn, button.btn" style="border-radius: 0px; font-size: 15px;">KNOW MORE</a>

